<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<GridPane alignment="CENTER"
          prefHeight="200"
          prefWidth="300"
          xmlns:fx="http://javafx.com/fxml/1"
          xmlns="http://javafx.com/javafx/8.0.112"
          fx:controller="com.FoodTracker.main.Scenes.Controllers.ConfirmationPopupController"
          hgap="10"
          vgap="10">
    <Text fx:id="text" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="2"/>
    <Button fx:id="trueButton" onAction="#confirm" prefHeight="25.0" prefWidth="100.0" text="yes"
            GridPane.rowIndex="2"/>
    <Button fx:id="falseButton" onAction="#confirm" prefHeight="25.0" prefWidth="100.0" text="no"
            GridPane.columnIndex="1" GridPane.rowIndex="2"/>

</GridPane>
